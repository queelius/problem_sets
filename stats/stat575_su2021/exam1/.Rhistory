logh <- function(y,alpha)
{
a <- floor(alpha)
b <- a / alpha
log(dgamma(y,shape=alpha,scale=1))-log(dgamma(y,shape=a,scale=b))
}
alpha <- 2.5
ys <- seq(0.00001,10,by=.001)
c <- exp(max(logh(ys,alpha)))
plot(x=ys,y=logh(ys,alpha))
cat("c =", c, "\n")
warnings()
